{
  "name": "messenger",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "predev": "yarn codegen",
    "prestart": "yarn codegen",
    "start": "next start",
    "sync": "node ./hooks/sync && yarn prettier --write ./gql > /dev/null",
    "sync:metadata": "node ./hooks/sync/metadata.js && yarn prettier --write ./gql/metadata.json",
    "sync:postgres": "node ./hooks/sync/postgres.js",
    "sync:schema:gql": "node ./hooks/sync/graphqurl/schema-as-gql.js && yarn prettier --write ./gql/schema.gql",
    "type:check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@bugsnag/js": "^7.14.1",
    "@perfective/common": "^0.8.0",
    "bootstrap": "5.1.3",
    "change-case": "^4.1.2",
    "chart.js": "^3.6.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.27.0",
    "emoji-picker-react": "^3.4.8",
    "faker": "^5.5.3",
    "feather-icons-react": "^0.5.0",
    "graphql": "^16.1.0",
    "graphqurl": "^1.0.1",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.6.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-bootstrap": "^2.0.3",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.4.2",
    "react-flatpickr": "^3.10.7",
    "react-highlight": "^0.14.0",
    "react-hook-form": "^7.21.0",
    "react-map-gl": "^6.1.18",
    "react-number-format": "^4.8.0",
    "react-quill": "^1.3.5",
    "react-select": "^5.2.1",
    "react-table": "^7.7.0",
    "react-textarea-autosize": "^8.3.3",
    "sass": "^1.44.0",
    "subscriptions-transport-ws": "^0.11.0",
    "ts-react-feather-icons": "^1.0.0",
    "underscore": "^1.13.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.0.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/near-operation-file-preset": "^2.2.2",
    "@graphql-codegen/typescript": "^2.4.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.8",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@graphql-tools/load": "^7.4.1",
    "@graphql-tools/utils": "^8.5.4",
    "@types/classnames": "^2.3.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-beautiful-dnd": "^13.1.0",
    "@types/underscore": "^1.11.4",
    "@types/uuid": "^8.3.3",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2"
  }
}
